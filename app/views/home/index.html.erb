<body>
<% if session.user %>
  <%= link_to "Log Out", logout_path %>
<% else %>
  <%= link_to "Create Account", new_user_path %>
  <%= link_to "Log In", login_form_path %>
<% end %>
  <section id="users">
    <h3>Existing Users</h3>
      <div id="create_user">
        <%= button_to "Create New User", "/register", method: :get %>
      </div>
      <br>
      <% @users.each do |user| %>
        <div id="user-<%= user.id %>">
          <%= link_to "#{user.email}", "/users/#{user.id}" %>
        </div>
      <br>
      <% end %>
  </section>
</body>