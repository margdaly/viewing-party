<body>
  <section id="users">
    <h3>Existing Users</h3>
      <% if current_user %>
      <div id="logout">
        <%= link_to "Logout", logout_path %>
      </div>
      <% else %>
      <div id="login">
        <%= link_to "Login", login_form_path %>
      </div>
      <% end %>
      <div id="create_user">
        <%= button_to "Create New User", "/register", method: :get %>
      </div>
      <br>
      <% if current_user %>
        <% @users.each do |user| %>
          <div id="user-<%= user.id %>">
            <%= "#{user.email}" %>
          </div>
        <br>
        <% end %>
      <% end %>
  </section>
</body>